<?xml version="1.0" ?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer.wishlist.items">
            <block class="Magento\Swatches\Block\Product\Renderer\Listing\Configurable" name="category.product.type.details.renderers.configurable" as="configurable" template="Magento_Swatches::product/listing/renderer.phtml" before="customer.wishlist.item.inner">
                <arguments>
                    <argument name="configurable_view_model" xsi:type="object">Magento\Swatches\ViewModel\Product\Renderer\Configurable</argument>
                </arguments>
            </block>
        </referenceBlock>
        <referenceBlock name="customer.wishlist.items">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">AumTechnolabs_MyWishlistWithSwatches::hyva/item/list.phtml</argument>
            </action>
        </referenceBlock>
    </body>
</page>
